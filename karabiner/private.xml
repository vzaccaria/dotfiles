        <item>
            <name>
                ⌃⇧n Copy a zotero link from Skim
            </name>
            <identifier>
                __n_copy_a_zotero_link_from_skim
            </identifier>
            <only>
                SKIM
            </only>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__Z__O
                </name>
                <url type="shell">
                    /usr/local/bin/node /usr/local/bin/skimk get -z -o
                </url>
            </vkopenurldef>
            <autogen>
                --KeyToKey-- KeyCode::N, ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L,KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__Z__O
