        <item>
            <name>
                ⌃⇧[mga] open (m)ail, (g)ithub or (a)ctivity monitor
            </name>
            <identifier>
                ___mga__open__m_ail___g_ithub_or__a_ctivity_monitor
            </identifier>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_WEB_HTTPS___GITHUB_COM_VZACCARIA
                </name>
                <url>
                    https://github.com/vzaccaria
                </url>
            </vkopenurldef>
            <autogen>
                --KeyToKey-- KeyCode::G, ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L,KeyCode::VK_OPEN_URL_WEB_HTTPS___GITHUB_COM_VZACCARIA
            </autogen>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_WEB_HTTPS___GMAIL_GOOGLE_COM
                </name>
                <url>
                    https://gmail.google.com
                </url>
            </vkopenurldef>
            <autogen>
                --KeyToKey-- KeyCode::M, ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L,KeyCode::VK_OPEN_URL_WEB_HTTPS___GMAIL_GOOGLE_COM
            </autogen>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_APP__APPLICATIONS_UTILITIES_ACTIVITY_MONITOR_APP
