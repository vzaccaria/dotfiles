        <item>
            <name>
                ⌃⇧n Copy a zotero link from Skim
            </name>
            <identifier>
                __n_copy_a_zotero_link_from_skim
            </identifier>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__Z__O
                </name>
                <url type="shell">
                    /usr/local/bin/node /usr/local/bin/skimk get -z -o
                </url>
            </vkopenurldef>
            <autogen>
                --KeyToKey-- KeyCode::N, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L,KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__Z__O
            </autogen>
            <appdef>
                <appname>
                    net_sourceforge_skim_app_skim
                </appname>
                <equal>
                    net.sourceforge.skim-app.skim
                </equal>
            </appdef>
            <only>
                net_sourceforge_skim_app_skim
            </only>
        </item>
