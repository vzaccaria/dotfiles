        <item>
            <name>
                Add shortcut to copy a note in skim through skimk
            </name>
            <identifier>
                add_shortcut_to_copy_a_note_in_skim_through_skimk
            </identifier>
            <only>
                SKIM
            </only>
            <vkopenurldef>
                <name>
                    KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__V___TEE____SKIMK
                </name>
                <url type="shell">
                    /usr/local/bin/node /usr/local/bin/skimk get -v | tee ~/.skimk
                </url>
            </vkopenurldef>
            <autogen>
                --KeyToKey-- KeyCode::N, ModifierFlag::CONTROL_L,KeyCode::VK_OPEN_URL_SHELL__USR_LOCAL_BIN_NODE__USR_LOCAL_BIN_SKIMK_GET__V___TEE____SKIMK
            </autogen>
