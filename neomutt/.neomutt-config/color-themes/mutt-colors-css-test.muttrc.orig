color index         bright-bf0020     dddddd "~s\"((u|U)rgent|URGENT)\""  



color index_tag		  orangered  dddddd      "attachment" 


color hdrdefault    888888 dddddd
color header        bright-000000     dddddd      "^(From)"
color header        bright-000000     dddddd      "^(Subject)"

color quoted        666666     dddddd
color signature     666666     dddddd

color body          bright-0b8800     dddddd "[;:][-o][)/(|]"    # emoticons
color body          bright-0b8800     dddddd "[;:][-][D]"    # emoticons
color body          bright-0b8800     dddddd "[;:][)(|]"         # emoticons

color sidebar_divider 999999 dddddd
